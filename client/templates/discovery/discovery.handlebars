<div class="container-fluid inner-discovery full-height" id="discovery">
  <div class="row-fluid centered-cell">
    <span class="span12">
      <div class="discovery-page-header discovery-transparent-cover">
        <center><h1>Find your organization to join a game</center></h1>
      </div>
    </span>
    <div class="row-fluid">
      <div class="span12">
        <div class="discovery-search">
          {{view Ember.TextField
          type="text"
          placeholder="Search"
          id="login-name"
          valueBinding="filterString"
        }}
      </div>
    </span>
    <div class="listItemContainer">
      {{#each orgs}}
      {{view App.ListItemView}}
      {{/each}}
    </div>
  </div>
</div>

