.event-feed-row
  height: 90px
  width: 300px
  line-height: 25px
  font-family: 'Nunito', sans-serif;
  font-size: 15px
  border-top: 1px solid #BFC5C5
  background-color: rgba(255, 249, 249, 0.58)
  float: right
  display: block
  padding-left: 15px
.join-event
  .shaddow
    box-shadow: 0 2px 6px 3px join-event-color;
    height: 102;
    width: 2px;
    background-color: join-event-color;
    position: absolute;
    left: -2;
  .top-inner-row
    height: 11px
    .join-time
      color: #918C8C;
      font-size: 10;
      position: absolute;
      margin-left: 77px;
  .middle-inner-row
    display: inline-block
    .join-photo
      float: left 
    .join-action
      margin-top: 9px;
      float: left
      width: 167px;
      font-weight: bold
      color: #413B3B
      font-size: 19px
      margin-left: 20px
      .join-underline
        margin-top: 6px;
        background-color: join-event-color;
        height: 2px;
        width: 158px;
    .join-arrow
      background: url(../img/join-arrow.png) no-repeat
      width: 50px;
      height: 53px;
      float: right;
.tag-event
  .shaddow
    box-shadow: 0 2px 6px 3px tag-event-color;
    height: 102;
    width: 2px;
    background-color: tag-event-color;
    position: absolute;
    left: -2;
  .top-inner-row
    height: 11px
    .tag-time
      color: #918C8C;
      font-size: 10;
      position: absolute;
      margin-left: 77px;
  .middle-inner-row
    display: inline-block
    .tag-photo
      float: left
    .tag-action
      margin-top: 9px;
      float: left
      font-weight: bold
      color: #413B3B
      font-size: 19px
      margin-left: 20px
      margin-right: 17px
      .tag-underline
        margin-top: 6px;
        background: url(../img/tag-arrow.png) no-repeat
        height: 10px;
        width: 85px;
.bottom-inner-row
  .taggee-name
    margin-left: 177px;
    position: absolute;
  .tagger-name
